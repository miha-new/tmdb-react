{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/vite"
    },
    {
      "src": "api/route.js",
      "use": "@vercel/edge"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "api/route.js"
    },
    {
      "src": "/.*",
      "dest": "index.html"
    }
  ]
}